{
  "expo": {
    "name": "5 Tenedores",
    "slug": "5-tenedores",
    "privacy": "public",
    "sdkVersion": "37.0.0",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#00A680"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.salvadoraragon.cincotenedores",
      "permissions": ["CAMERA_ROLL", "LOCATION"]
    },
    "description": "Aplicación de valoración de restaurantes",
    "githubUrl": "https://github.com/Salvaaragon/5-tenedores"
  }
}
